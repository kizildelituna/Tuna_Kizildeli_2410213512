#include <stdio.h>

int adj[20][20];
int visited[20];
int queue[20];
int n;
int front = 0, rear = -1;

void dfs(int v) {
    int i;
    visited[v] = 1;
    printf("%d ", v);
    for (i = 0; i < n; i++) {
        if (adj[v][i] == 1 && !visited[i]) {
            dfs(i);
        }
    }
}

void bfs(int v) {
    int i;
    for (i = 0; i < n; i++) visited[i] = 0;
    
    visited[v] = 1;
    queue[++rear] = v;
    
    while (front <= rear) {
        int current = queue[front++];
        printf("%d ", current);
        
        for (i = 0; i < n; i++) {
            if (adj[current][i] == 1 && !visited[i]) {
                visited[i] = 1;
                queue[++rear] = i;
            }
        }
    }
}

int main() {
    int i, j, start;
    
    printf("Enter number of vertices: ");
    scanf("%d", &n);
    
    printf("Enter adjacency matrix:\n");
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            scanf("%d", &adj[i][j]);
        }
    }
    
    printf("Enter starting vertex: ");
    scanf("%d", &start);
    
    for (i = 0; i < n; i++) visited[i] = 0;
    
    printf("\nDFS result: ");
    dfs(start);
    
    printf("\nBFS result: ");
    bfs(start);
    
    return 0;
}
