#include <stdio.h>

#define MAX 100

int graph[MAX][MAX];
int visited[MAX];
int n;

void dfs(int v, int cluster[]) {
    visited[v] = 1;
    cluster[v] = 1;

    for (int i = 0; i < n; i++) {
        if (graph[v][i] == 1 && !visited[i]) {
            dfs(i, cluster);
        }
    }
}

int main() {
    printf("dugum sayisi: ");
    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &graph[i][j]);
        }
    }

    for (int i = 0; i < n; i++) visited[i] = 0;

    int clusterId = 0;

    for (int i = 0; i < n; i++) {
        if (!visited[i]) {
            int cluster[MAX] = {0};
            clusterId++;

            dfs(i, cluster);

            printf("cluster %d: ", clusterId);
            for (int j = 0; j < n; j++) {
                if (cluster[j]) printf("%d ", j);
            }
            printf("\n");
        }
    }

    return 0;
}
