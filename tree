#include <stdio.h>

#define MAX 100

int tree[MAX];
int size = 0;

void insert(int x) {
    if (size < MAX) {
        tree[size++] = x;
    }
}

void printLevelOrder() {
    for (int i = 0; i < size; i++) {
        printf("%d ", tree[i]);
    }
    printf("\n");
}

void inorder(int i) {
    if (i >= size) return;
    inorder(2*i + 1);
    printf("%d ", tree[i]);
    inorder(2*i + 2);
}

void preorder(int i) {
    if (i >= size) return;
    printf("%d ", tree[i]);
    preorder(2*i + 1);
    preorder(2*i + 2);
}

void postorder(int i) {
    if (i >= size) return;
    postorder(2*i + 1);
    postorder(2*i + 2);
    printf("%d ", tree[i]);
}

int main() {
    int n, x;

    printf("kac dugum: ");
    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        scanf("%d", &x);
        insert(x);
    }

    printf("Tree (level order): ");
    printLevelOrder();

    printf("Inorder: ");
    inorder(0);
    printf("\n");

    printf("Preorder: ");
    preorder(0);
    printf("\n");

    printf("Postorder: ");
    postorder(0);
    printf("\n");

    return 0;
}
